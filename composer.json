{
    "name": "qvalent/yii2-comments",
    "description": "Yii2 comments",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "qvalent",
            "email": "valentin531@yandex.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "hakimel/Ladda": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "qvalent\\comments\\": ""
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "hakimel/Ladda",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/hakimel/Ladda",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "extra": {
        "bootstrap": "qvalent\\comments\\Bootstrap"
    }
}
